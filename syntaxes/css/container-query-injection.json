{
  "scopeName": "container-query-injection.injection",
  "injectionSelector": ["L:meta.at-rule.header.css", "L:meta.at-rule.media.header.css"],
  "patterns": [
    {
      "include": "#min-width"
    },
    {
      "include": "#max-width"
    },
    {
      "include": "#width"
    },
    {
      "include": "#constant"
    }
  ],
  "repository": {
    "min-width": {
      "name": "support.type.property-name.css",
      "match": "min-width"
    },
    "max-width": {
      "name": "support.type.property-name.css",
      "match": "max-width"
    },
    "width": {
      "name": "support.type.property-name.css",
      "match": "width"
    },
    "constant": {
      "name": "constant.numeric.css",
      "match": "1|2|3|4|5|6|7|8|9|0"
    }
  }
}