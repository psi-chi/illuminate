{
  "name": "illuminate",
  "displayName": "Illuminate",
  "version": "1.2.0",
  "publisher": "psi-chi",
  "description": "A color theme for Visual Studio Code with luminous syntax and comments highlighting.",
  "license": "MIT",
  "engines": {
    "vscode": "^1.72.0"
  },
  "author": {
    "name": "Psi"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#373a40",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psi-chi/illuminate"
  },
  "bugs": {
    "url": "https://github.com/psi-chi/illuminate/issues"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "color-theme",
    "comments",
    "dark",
    "illuminate",
    "theme"
  ],
  "contributes": {
		"css": {
			"customData": [
				"custom/css.json"
			]
		},
    "grammars": [
      {
        "injectTo": [
          "source.css",
          "source.svelte"
        ],
        "scopeName": "at-rule-property-injection.injection",
        "path": "syntaxes/css/at-rule-property-injection.json"
      },
      {
        "injectTo": [
          "source.css",
          "source.svelte"
        ],
        "scopeName": "container-query-injection.injection",
        "path": "syntaxes/css/container-query-injection.json"
      },
      {
        "injectTo": [
          "text.html.derivative"
        ],
        "scopeName": "doctype-injection.injection",
        "path": "syntaxes/html/doctype-injection.json"
      },
      {
        "injectTo": [
          "text.html.derivative",
          "source"
        ],
        "scopeName": "comment-injection.injection",
        "path": "syntaxes/universal/comment-injection.json"
      },
      {
        "injectTo": [
          "text.html.derivative",
          "source"
        ],
        "scopeName": "dash-comment-injection.injection",
        "path": "syntaxes/universal/dash-comment-injection.json"
      }
    ],
    "themes": [
      {
        "label": "Illuminate",
        "uiTheme": "vs-dark",
        "path": "themes/Illuminate-color-theme.json"
      }
    ]
  }
}
