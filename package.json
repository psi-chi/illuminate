{
  "name": "illuminate",
  "displayName": "Illuminate",
  "version": "0.4.0",
  "publisher": "psi-chi",
  "description": "A theme with luminous syntax highlighting.",
  "license": "MIT",
  "engines": {
    "vscode": "^1.70.0"
  },
  "author": {
    "name": "Psi"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#202225",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psi-chi/illuminate"
  },
  "bugs": {
    "url": "https://github.com/psi-chi/illuminate/issues"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "color-theme",
    "dark",
    "illuminate",
    "theme"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Illuminate",
        "uiTheme": "vs-dark",
        "path": "./themes/Illuminate-color-theme.json"
      }
    ],
    "grammars": [
      {
        "injectTo": [
          "text.html.derivative",
          "source"
        ],
        "scopeName": "comment-injection.injection",
        "path": "./syntaxes/comment-injection.json"
      },
      {
        "injectTo": [
          "text.html.derivative"
        ],
        "scopeName": "html-doctype-injection.injection",
        "path": "./syntaxes/html-doctype-injection.json"
      }
    ]
  }
}
